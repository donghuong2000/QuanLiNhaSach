@model QuanLiNhaSach.Models.Receipt
@{
    ViewData["Title"] = "Chi tiết phiếu thu tiền";
}
<h1 class="text-center">Chi tiết phiếu thu tiền</h1>


<hr />
<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
        <form   enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label class="control-label">Mã thu tiền</label>
                <input type="text" asp-for="Id" class="form-control" readonly />        
            </div>
            <div class="form-group">
                <label class="control-label">Tên khách hàng</label>
                <input type="text" asp-for="ApplicationUser.FullName" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label class="control-label">Địa chỉ</label>
                <input asp-for="ApplicationUser.Address" type="text" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label class="control-label">Điện thoại</label>
                <input asp-for="ApplicationUser.PhoneNumber" type="text" class="form-control"  readonly />
            </div>
            <div class="form-group">
                <label class="control-label">Email</label>
                <input asp-for="ApplicationUser.Email" type="text" class="form-control"  readonly />
            </div>
            <div class="form-group">
                <label class="control-label">Ngày thu tiền</label>
                <input type="datetime-local" class="form-control"  asp-for="DateCreate" readonly />
            </div>
            <div class="form-group">
                <label asp-for="Proceeds" class="control-label">Số tiền thu</label>
                <input type="number" asp-for="Proceeds"  class="form-control" readonly/>
            </div>
        </form>
    </div>
</div>



@section Scripts{
    <script src="~/Admin/js/receipt.js"></script>
}